load("//devtools/python/blaze:pytype.bzl",  "pytype_strict_library")provider
package(default_visibility = ["//third_party/py/scenic:scenic_friends"])

licenses(["notice"])

pytype_strict_library(
    name = "base_models",
    srcs = [
        "base_model.py",
        "classification_model.py",
        "encoder_decoder_model.py",
        "multilabel_classification_model.py",
        "segmentation_model.py",
        "set_detection_model.py",
    ],
    srcs_version = "PY3",
    deps = [
        ":model_utils",
        "//third_party/py/absl/logging",
        "//third_party/py/flax/training",
        "//third_party/py/jax",
        "//third_party/py/numpy",
    ],
)

py_library(
    name = "model_utils",
    srcs = ["model_utils.py"],
    srcs_version = "PY3",
    deps = [
        "//third_party/py/absl/logging",
        "//third_party/py/flax:core",
        "//third_party/py/jax",
        "//third_party/py/numpy",
    ],
)
